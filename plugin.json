{
	"id": "nodebb-plugin-quickstart",
	"url": "https://github.com/NodeBB/nodebb-plugin-quickstart",
	"library": "./library.js",
	"hooks": [{
			"hook": "static:app.load",
			"method": "init"
		}, {
			"hook": "filter:teasers.get",
			"method": "addScripts"
		},
		{
			"hook": "filter:admin.header.build",
			"method": "addAdminNavigation"
		}, {
			"hook": "filter:header.build",
			"method": "addNavigation"
		},
		{
			"hook": "action:post.save",
			"method": "beforePostContent"
		}
	],
	"staticDirs": {
		"static": "./static"
	},
	"less": [
		"static/style.less"
	],
	"scripts": [
		"static/lib/main.js"
	],
	"acpScripts": [
		"static/lib/admin.js"
	],
	"templates": "static/templates"
}
